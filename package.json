{
  "name": "eatonfyi",
  "version": "1.0.0",
  "description": "Eaton FYI Eleventy site",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "start": "npm run dev-cms",
    "dev": "run-s clean eleventy:dev --print-label",
    "dev-cms": "run-p cms dev",
    "build": "run-s clean eleventy:prod --print-label",
    "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
    "cms": "netlify-cms-proxy-server",
    "clean": "del-cli dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eaton/eatonfyi.git"
  },
  "keywords": [
    "eleventy"
  ],
  "author": "Jeff Eaton",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "del-cli": "^3.0.1",
    "luxon": "^1.24.1",
    "markdown-it": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "elasticlunr": "^0.9.5"
  },
  "devDependencies": {
    "npx": "^10.2.2",
    "netlify-cms-proxy-server": "^1.3.6",
    "cross-env": "^7.0.2"
  },
  "bugs": {
    "url": "https://github.com/eaton/eatonfyi/issues"
  },
  "homepage": "https://github.com/eaton/eatonfyi#readme"
}
